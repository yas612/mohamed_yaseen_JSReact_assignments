//js file

var a = [1,3,6,4];var l;var sum=0;
function forloop(a){
  for(l=0;l<a.length;l++){
    sum=sum+a[l];
  }
  return sum;
}
document.write("The sum for the forloop :"+forloop(a)+"<br>");

function whileloop(a){
  sum=0;
  var l = a.length-1;
  while(l>=0){
    sum=sum+a[l];
    l--;

  }
  return sum;
}
document.write("The sum for the whileloop :"+whileloop(a)+"<br>");

