class Rectangle{
    constructor(height , width){
        this.height= height;
        this.width=width;
    }
     get area(){
         return this.width*this.height;
     }
}

var rectangle1= new Rectangle(10,5);
var rectangle2=new Rectangle(20,15);

document.write("rectangle1 area is"+ rectangle1.area);
document.write("rectangle2 area is "+rectangle2.area);
