public class Application implements Comparable<Application>
{
    int id;
    String name;
    String department;
    int salary;
    public Application(int id,String name,String department,int salary)
    {
        this.id=id;
        this.name=name;
        this.department=department;
        this.salary=salary;

    }
    public int compareTo(Application o)
    {
        if(name.compareTo(o.name)>0)
        {
            return 1;

        }
        else if(name.compareTo(o.name)<0)
        {
            return -1;
        }
        else
            return 0;

    }
}
