public class Single
{
   public static void main(String[] args)
   {
       String str1="java String pool refers collection of strings which are stored in heap memory";

       System.out.println(str1.toUpperCase());
       System.out.println(str1.toLowerCase());
       System.out.println(str1.replace('a', '$'));
       System.out.println(str1.contains("collection"));
       System.out.println(str1.equals("java string pool refers collection of strings which are stored in heap memory"));
       System.out.println(str1.equalsIgnoreCase("java string pool refers collection of strings which are stored in heap memory"));
   }
}
