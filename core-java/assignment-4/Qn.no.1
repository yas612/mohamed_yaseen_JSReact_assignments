import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
        Scanner in = new Scanner(System.in);
        int a=in.nextInt();
        int b=in.nextInt();
        int c=a/b;
        try
        {
          c=a/0;   
        }
        catch(ArithmeticException e)
        {
            System.out.println(e);
            System.out.println("error occurs");
        }
        System.out.println(c);
    }
}
