public @interface Info
{
   int  Author_Id();
    String date();
    String time();
    double version();
}
@Info(Author_Id = 8521,date = "01-11-2021",time="10:10:21",version = 3.02)
public class Class1
{

   public Class1(int Author_ID,String date,String time,double version)
   {
       this.Author_Id=Author_ID;
       this.date=date;
       this.time=time;
       this.version=version;
   }
   int Author_Id;
   String date;
   String time;
   double version;
}
public class Class2
{
    public static void main(String[] args)
    {
        Class1 a = new Class1(8521,"10-10-2021","10:10:21",3.02);
        System.out.println("author id is: "+a.Author_Id);
        System.out.println("date is :"+a.date);
        System.out.println("time is :"+a.time);
        System.out.println("version is :"+a.version);
    }
}
