import java.util.HashMap;
public class Pair<k,v>
{
    public Pair(k key,v value)
    {
      this.key=key;
      this.value=value;
    }
   k key;
    v value;
    public <k,v> String gene()
    {
        return key+" "+value;
    }

}

public class New {
    public static void main(String[] args) {
        Pair<String, String> ob1 = new Pair<String, String>("kohli", "captain");
        System.out.println(ob1.gene());

    }
}

